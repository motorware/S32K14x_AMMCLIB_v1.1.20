        %assign p1_val = LibBlockParameter(p1, "", "", 0)
        %assign p2_val = LibBlockParameter(p2, "", "", 0)
        %% 
        /* %<Type> (%<ParamSettings.FunctionName>): %<Name> */
        AMCLIB_CurrentLoop_SF_FLT_wrapper_Output(%<u1_val>, %<work1_ptr>, %<work1BUS_ptr>, %<work2_ptr>, %<work2BUS_ptr>, %<u2_ptr>, %<u2BUS_ptr>, %<u3_ptr>, %<u3BUS_ptr>, %<work3_ptr>, %<u4_val>, %<u5_val>, %<u6_val>, %<u7_val>, %<u8_val>, %<u9_val>, %<u10_val>, %<u11_val>, %<u12_val>, %<u13_val>, %<y1_ptr>, %<y1BUS_ptr>, %<y2_ptr>, %<y3_ptr>, %<y4_ptr>, %<y5_ptr>, %<p1_val>, %<p2_val>);
    %else
        %assign fltUDcBus = LibBlockInputSignal(0, "", "", 0)
        %assign pPIrAWD = LibBlockDWork(work1, "", "", 0)
        %assign pPIrAWQ = LibBlockDWork(work2, "", "", 0)
        %assign pIDQReq = LibBlockInputSignalAddr(1, "", "", 0)
        %assign pIDQFbck = LibBlockInputSignalAddr(2, "", "", 0)
        %assign s32ResetStates = LibBlockDWork(work3, "", "", 0)
        %assign fltControllerPIrAWDOut = LibBlockInputSignal(3, "", "", 0)
        %assign fltControllerPIrAWQOut = LibBlockInputSignal(4, "", "", 0)
        %assign fltCC1sc_D = LibBlockInputSignal(5, "", "", 0)
        %assign fltCC2sc_D = LibBlockInputSignal(6, "", "", 0)
        %assign fltCC1sc_Q = LibBlockInputSignal(7, "", "", 0)
        %assign fltCC2sc_Q = LibBlockInputSignal(8, "", "", 0)
        %assign fltAcc_D = LibBlockInputSignal(9, "", "", 0)
        %assign fltInErrK1_D = LibBlockInputSignal(10, "", "", 0)
        %assign fltAcc_Q = LibBlockInputSignal(11, "", "", 0)
        %assign fltInErrK1_Q = LibBlockInputSignal(12, "", "", 0)
        %assign pUDQReq = LibBlockOutputSignalAddr(0, "", "", 0)
        %assign fltAcc_D_Out = LibBlockOutputSignal(1, "", "", 0)
        %assign fltInErrK1_D_Out = LibBlockOutputSignal(2, "", "", 0)
        %assign fltAcc_Q_Out = LibBlockOutputSignal(3, "", "", 0)
        %assign fltInErrK1_Q_Out = LibBlockOutputSignal(4, "", "", 0)
        %assign showStates = LibBlockParameter(p1, "", "", 0)
        %assign resetStatesEnable = LibBlockParameter(p2, "", "", 0)
        %% 
        %if FEVAL("strcmp", showStates, "2")
          /* Copy inputs to struct states*/
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWD.fltCC1sc = (tFloat)(%<fltCC1sc_D>);
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWD.fltCC2sc = (tFloat)(%<fltCC2sc_D>);
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWD.fltAcc = (tFloat)(%<fltAcc_D>);
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWD.fltInErrK1 = (tFloat)(%<fltInErrK1_D>);
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWQ.fltCC1sc = (tFloat)(%<fltCC1sc_Q>);
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWQ.fltCC2sc = (tFloat)(%<fltCC2sc_Q>);
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWQ.fltAcc = (tFloat)(%<fltAcc_Q>);
          fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWQ.fltInErrK1 = (tFloat)(%<fltInErrK1_Q>);
        %endif
        
        /* Copy inputs to struct states*/
        fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pIDQReq = (SWLIBS_2Syst_FLT *)(%<pIDQReq>);
        fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pIDQFbck = (SWLIBS_2Syst_FLT *)(%<pIDQFbck>);
        
        %if FEVAL("strcmp", resetStatesEnable, "1")
          if ((tS32)(%<s32ResetStates>) != (tS32)0)
          {
            %<s32ResetStates> = (tS32)0;
            AMCLIB_CurrentLoopSetState_FLT((tFloat)(%<fltControllerPIrAWDOut>), (tFloat)(%<fltControllerPIrAWQOut>), &fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>);
            
            /* Set outputs to zero */
            (%<pUDQReq>)->fltArg1 = 0.0f;
            (%<pUDQReq>)->fltArg2 = 0.0f;
          }
          else
          {
            AMCLIB_CurrentLoop_FLT((tFloat)(%<fltUDcBus>), (SWLIBS_2Syst_FLT *)(%<pUDQReq>), &fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>);
          }
        %else
          AMCLIB_CurrentLoop_FLT((tFloat)(%<fltUDcBus>), (SWLIBS_2Syst_FLT *)(%<pUDQReq>), &fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>);
        %endif
        
        %if FEVAL("strcmp", showStates, "2")
          /* Copy struct states to outputs */
          %<fltAcc_D_Out> = fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWD.fltAcc;
          %<fltInErrK1_D_Out> = fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWD.fltInErrK1;
          %<fltAcc_Q_Out> = fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWQ.fltAcc;
          %<fltInErrK1_Q_Out> = fltCurrentLoopStateStr_%<SYSNAME(Name)[0]>.pPIrAWQ.fltInErrK1;
        %endif
    %endif
%endfunction

%% Function: Terminate ====================================================
%function Terminate (block, system) Output
    %if IsModelReferenceSimTarget() || CodeFormat=="S-Function" || ::isRAccel
        %assign y1BUSBUS_addr = "&" + LibBlockPWork("", "", "", 0)
        %assign u2BUSBUS_addr = "&" + LibBlockPWork("", "", "", 1)
        %assign u3BUSBUS_addr = "&" + LibBlockPWork("", "", "", 2)
        %assign work1BUSBUS_addr = "&" + LibBlockPWork("", "", "", 3)
        %assign work2BUSBUS_addr = "&" + LibBlockPWork("", "", "", 4)
        %assign blockPath = STRING(LibGetBlockPath(block))
        if (AMCLIB_CurrentLoop_SF_FLT_wrapper_freemem(%<y1BUSBUS_addr>, %<u2BUSBUS_addr>, %<u3BUSBUS_addr>, %<work1BUSBUS_addr>, %<work2BUSBUS_addr>)!=0) %<LibSetRTModelErrorStatus("\"Memory free failure for %<blockPath>\"")>;
    %else
    %endif
%endfunction

